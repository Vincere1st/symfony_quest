{% extends 'base.html.twig' %}

{% block body %}
    <table>
        <tbody>
        {% for article in articles %}
            <tr>
                <td>{{ article.id }}</td>
                <td>{{ article.title }}</td>
                <td>{{ article.content }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
